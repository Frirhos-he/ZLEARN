                <ui:Toggle label="Sound" name="SoundToggle" style="margin-top: 0; margin-right: 0; margin-bottom: 50px; margin-left: 0; width: 100%;" />
                <ui:Slider picking-mode="Ignore" label="Music volume" value="42" high-value="100" name="VolumeSlider" style="width: 70%; margin-top: 0; margin-right: 0; margin-bottom: 50px; margin-left: 0;" />
                <ui:Toggle label="Vibration" name="Vibration" style="margin-top: 0; margin-right: 0; margin-bottom: 50px; margin-left: 0; width: 100%;" />
                <ui:Toggle label="AR uses planes" name="ARSettingsToggle" style="width: 100%;" />
                <ui:Button text="RESET DATA" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ResetSaveFileButton" class="button-primary" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
